cluster-role version="rbac.authorization.k8s.io/v1" name="system:kube-dns-autoscaler" {
  rule {
    resources "nodes"
    verbs "list" "watch"
  }
  rule {
    resources "replication-controllers/scale"
    verbs "get" "update"
  }
  rule {
    api-groups "apps"
    resources "deployments/scale" "replica-sets/scale"
    verbs "get" "update"
  }
  rule {
    resources "config-maps"
    verbs "get" "create"
  }
}
