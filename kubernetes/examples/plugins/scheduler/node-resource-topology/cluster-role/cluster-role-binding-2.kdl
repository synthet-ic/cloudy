api-version = "rbac.authorization.k8s.io/v1"
kind = "ClusterRoleBinding"

[metadata]
name = "my-scheduler-as-volume-scheduler"

[[subjects]]
kind = "ServiceAccount"
name = "my-scheduler"
namespace = "kube-system"

[roleRef]
kind = "ClusterRole"
name = "system:volume-scheduler"
apiGroup = "rbac.authorization.k8s.io"
